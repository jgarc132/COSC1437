public class HandsOnActivity{
  public static void main(String[] args){
    
  
  
  }
  public static String insertDot(String s){
      
      return s;
  }
  public static  int repeatedElements(int[] a, int[] a,int n){
    return repeatedElements(a,b,n,0);
  }
  private static int repeatedElements(int[] a, int[] b, int n, int c){
    if(c > a.length && c > b.length)
      return c; 
    else if(n == a[c])
      n++;
    else if(n == b[c])
      n++;
    else
      repeatedElements(a,b,n,c++);
  }
}